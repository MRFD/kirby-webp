{
  "name": "mrfd/kirby-webp",
  "version": "1.0.2",
  "description": "Kirby 3 plugin for converting images to WebP on the fly, with support for file methods and Kirby tags!",
  "type": "kirby-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "Marijn Roovers",
      "email": "marijn@mrfd.nl"
    }
  ],
  "keywords": [
    "kirby3",
    "kirby3-cms",
    "kirby3-plugin"
  ],
  "require": {
    "php": ">=7.2",
    "rosell-dk/webp-convert": "^2.3",
    "getkirby/composer-installer": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "MRFD\\WebP\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true
  }
}
